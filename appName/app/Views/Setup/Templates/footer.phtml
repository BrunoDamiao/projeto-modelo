  <script>
  jQuery(function($){


    $( ".nav-link" ).click(function() {
      var data  = $(this).attr('id');
      // alert('oi '+data);
      $( "#DBDrive" ).prop("value", data);
    });

    $( "#btnCon" ).click(function() {
      $( "#pnlCon" ).removeClass('d-none').slideDown();
      $( "#pnlUser" ).slideUp().addClass('d-none');
      $( "#pnlProject" ).slideUp().addClass('d-none');

      $( "#btnCon" ).removeClass('btn-info');
      $( "#btnCon" ).addClass('btn-primary');

      $( "#btnUser" ).removeClass('btn-info');
      $( "#btnUser" ).removeClass('btn-primary');

      $( "#btnProject" ).removeClass('btn-info');
      $( "#btnProject" ).removeClass('btn-primary');

      $( "#btnUser" ).addClass('btn-secondary');
      $( "#btnProject" ).addClass('btn-secondary');

      $( "#btnSave" ).removeClass('btn-success');
      $( "#btnSave" ).addClass('btn-secondary');
      $( "#btnSave" ).prop("type", "button");
    });

    $( "#btnUser" ).click(function() {
      $( "#pnlUser" ).removeClass('d-none').slideDown();
      $( "#pnlCon" ).slideUp().addClass('d-none');
      $( "#pnlProject" ).slideUp().addClass('d-none');

      $( "#btnUser" ).removeClass('btn-secondary');
      $( "#btnUser" ).addClass('btn-primary');

      $( "#btnCon" ).removeClass('btn-info');

      $( "#btnCon" ).addClass('btn-info');
      $( "#btnProject" ).addClass('btn-info');

      $( "#btnSave" ).removeClass('btn-secondary');
      $( "#btnSave" ).addClass('btn-success');
      $( "#btnSave" ).prop("type", "submit");
    });

    $( "#btnProject" ).click(function() {
      $( "#pnlProject" ).removeClass('d-none').slideDown();
      $( "#pnlCon" ).slideUp().addClass('d-none');
      $( "#pnlUser" ).slideUp().addClass('d-none');

      $( "#btnProject" ).removeClass('btn-secondary');
      $( "#btnProject" ).removeClass('btn-info');
      $( "#btnProject" ).addClass('btn-primary');

      $( "#btnCon" ).removeClass('btn-primary');
      $( "#btnUser" ).removeClass('btn-primary');

      $( "#btnCon" ).addClass('btn-info');
      $( "#btnUser" ).addClass('btn-secondary');

      $( "#btnSave" ).removeClass('btn-success');
      $( "#btnSave" ).addClass('btn-secondary');
      $( "#btnSave" ).prop("type", "button");
    });

  });

  </script>

